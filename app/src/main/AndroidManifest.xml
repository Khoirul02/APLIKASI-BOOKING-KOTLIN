<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.huda.booking">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".activity.FaqActivity"></activity>
        <activity android:name=".activity.DaftarBookingPenggunaActivity" />
        <activity android:name=".activity.BookingPenggunaActivity" />
        <activity android:name=".activity.BookingActivity" />
        <activity android:name=".dashboard.DashboardUmumActivity" />
        <activity android:name=".activity.EditPenggunaActivity" />
        <activity android:name=".activity.PenggunaActivity" />
        <activity android:name=".activity.EditKamarActivity" />
        <activity android:name=".activity.KamarActivity" />
        <activity android:name=".RegistrasiActivity" />
        <activity android:name=".dashboard.DashboardPemilikActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>